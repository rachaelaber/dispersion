@article{wang_metagenomic_2016,
	title = {Metagenomic analysis of viral genetic diversity in respiratory samples from children with severe acute respiratory infection in {China}},
	volume = {22},
	issn = {1198743X},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1198743X16000173},
	doi = {10.1016/j.cmi.2016.01.006},
	abstract = {Severe acute respiratory infection (SARI) in children is thought to be mainly caused by infection with various viruses, some of which have been well characterized; however, analyses of respiratory tract viromes among children with SARI versus those without are limited. In this study, nasopharyngeal swabs from children with and without SARI (135 versus 15) were collected in China between 2008 and 2010 and subjected to multiplex metagenomic analyses using a next-generation sequencing platform. The results show that members of the Paramyxoviridae, Coronaviridae, Parvoviridae, Orthomyxoviridae, Picornaviridae, Anelloviridae and Adenoviridae families represented the most abundant species identiﬁed ({\textgreater}50\% genome coverage) in the respiratory tracts of children with SARI. The viral population found in the respiratory tracts of children without SARI was less diverse and mainly dominated by the Anelloviridae family with only a small proportion of common epidemic respiratory viruses. Several almost complete viral genomes were assembled, and the genetic diversity was determined among several samples based on next-generation sequencing. This research provides comprehensive mapping of the viromes of children with SARI and indicates high heterogeneity of known viruses present in the childhood respiratory tract, which may beneﬁt the detection and prevention of respiratory disease.},
	language = {en},
	number = {5},
	urldate = {2021-08-02},
	journal = {Clinical Microbiology and Infection},
	author = {Wang, Y. and Zhu, N. and Li, Y. and Lu, R. and Wang, H. and Liu, G. and Zou, X. and Xie, Z. and Tan, W.},
	month = may,
	year = {2016},
	pages = {458.e1--458.e9},
	annote = {- in this study, 135 children (samples into nine pools) with SARI and 15 without
- non-SARI viromes less diverse and dominated by Anelloviridae
 
Procedure:
1. VTM filtered through 0.22 micron filter
2. Filtrates were then centrifuged at 30,000 rpm for three hours to pellet the viral particles
3. Pellets treated with DNAse, RNAse, nuclease to get rid of any extracellular nucleic acids
4. Nucleic acid extraction using QIAmp MinElute Virus Spin Kit
5. DNA and RNA amplified by SISPA
6. HiSeq 2x 125-bp reads
7. Non-human reads assembled before searched against database
8. They separate viruses with {\textgreater} 50\% genome coverage and then compare groups with and without SARI},
