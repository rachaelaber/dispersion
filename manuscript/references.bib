	keywords = {consumer–resource interactions, disturbance, ecological models, ecological processes, effect size, experimental design, interaction webs, mean intensity cf. variance of ecological processes, patterns, random vs. fixed effects in ecological experiments, simulations and trophic interactions, temporal and spatial, variance vs. frequency},
	pages = {2335--2346},
	file = {Full Text PDF:/Users/rachaelaber/Zotero/storage/6UREK66V/Benedetti-Cecchi - 2003 - The Importance of the Variance Around the Mean Eff.pdf:application/pdf;Snapshot:/Users/rachaelaber/Zotero/storage/ZKFPUZ55/02-8011.html:text/html},
