	keywords = {Bayesian analysis, fossil pollen, measles, observation error, population regulation, state-space model, stochasticity, time-series analysis},
	pages = {3140--3150},
	file = {Snapshot:/Users/rachaelaber/Zotero/storage/IY8N4BJG/03-0520.html:text/html},
